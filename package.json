{
  "name": "@rezonant/execution-context",
  "version": "0.1.4",
  "description": "",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "nyc": {
    "all": true,
    "extension": [
      ".js",
      ".ts"
    ],
    "include": [
      "dist/**/*.js",
      "src/**/*.ts"
    ],
    "exclude": [
      "dist/**/*.test.js",
      "src/**/*.test.ts",
      "src/example.ts",
      "src/test.ts",
      "src/privileged.ts"
    ],
    "reporter": [
      "html"
    ]
  },
  "scripts": {
    "test": "npm run build && nyc node dist/test.js",
    "prepare": "npm run build",
    "build": "tsc -p .",
    "build:spec": "ecmarkup spec.html dist/spec.html",
    "start": "npm run build && node dist/example"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.3",
    "@types/node": "^12.7.8",
    "chai": "^4.2.0",
    "nyc": "^14.1.1",
    "razmin": "^0.6.10",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "ecmarkup": "^3.16.0",
    "zone.js": "^0.10.2"
  }
}
